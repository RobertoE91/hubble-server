{
    "plugins": ["jest"],
    "env": {
        "node": true,
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": ["eslint:recommended", "plugin:jest/recommended"],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-unused-vars": ["warn"],
        "no-mixed-spaces-and-tabs": ["warn"]
    }
}
