{
    "name": "hubble-server",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "nodemon --exec \"yarn lint && node ./app/index.js\"",
        "run": "node ./app/index.js",
        "pretty": "yarn pretty-quick",
        "lint": "yarn eslint app/**/*.js"
    },
    "dependencies": {
        "bcrypt": "^5.0.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "~4.16.1",
        "express-jwt": "^6.0.0",
        "joi": "^17.1.1",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.9.27",
        "morgan": "~1.9.1"
    },
    "devDependencies": {
        "eslint": "^7.5.0",
        "husky": "^4.2.5",
        "nodemon": "^2.0.4",
        "prettier": "^2.0.5",
        "pretty-quick": "^2.0.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "nodemonConfig": {
        "ignore": [
            "node_modules/*"
        ]
    }
}
